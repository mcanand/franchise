<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_web_link_category_form" model="ir.ui.view">
        <field name="name">web links categories</field>
        <field name="model">web.links.categories</field>
        <field name="arch" type="xml">
            <form string="web Links category">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_web_link_category_list" model="ir.ui.view">
        <field name="name">web links categories</field>
        <field name="model">web.links.categories</field>
        <field name="arch" type="xml">
            <tree string="web Links category" editable="top">
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="action_view_web_links_category" model="ir.actions.act_window">
        <field name="name">web link categories</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">web.links.categories</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_country_state_district_corparation_tree" model="ir.ui.view">
            <field name="name">res.country.state.district.corparation.tree</field>
            <field name="model">res.country.state.district.corparation</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="code"/>
                    <field name="district_id"/>
                </tree>
            </field>
        </record>
        <record id="view_country_state_district_corparation_form" model="ir.ui.view">
            <field name="name">res.country.state.district.corparation.form</field>
            <field name="model">res.country.state.district.corparation</field>
            <field name="arch" type="xml">
                <form string="Corparation">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="code"/>
                            <field name="district_id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_country_state_district_corparation" model="ir.actions.act_window">
            <field name="name">Corparation</field>
            <field name="res_model">res.country.state.district.corparation</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create a Corparation
                </p>
            </field>
        </record>

        <menuitem id="res_country_state_district_corparation_menu" name="Corparation"
                  action="action_country_state_district_corparation" parent="contacts.menu_localisation"
                  sequence="11"/>
    </data>
</odoo>